<% layout('/layouts/boilerplate') %>
    <h1>All Campgrounds</h1>
    <div><a href="/campgrounds/new">ADD A NEW CAMPGROUND</a></div>
    <ul>
        <% for(let campground of campgrounds) {%>
            <li>
                <a href="/campgrounds/<%= campground.id %> ">
                    <%= campground.title %>
                </a>
            </li>
            <% } %>
    </ul>